<div class="album py-5">
  <div class="container-fluid">
    <%= render 'lands/buttons_search' %>

    <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4 g-3" id="lands">
      <% @lands.each do |land| %>
        <%= render land %>
      <% end %>
    </div>

    <div class="d-flex justify-content-center">
      <%= paginate @lands %>
    </div>

  </div>
</div>

